{
  "name": "microbiology-db",
  "version": "1.0.1",
  "description": "System to store sample records and generate reports for microbiology department",
  "main": "main.js",
  "build": {
    "icon": "build/icon.ico"
  },
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
