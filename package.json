    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "ejs": "^3.1.6",
    "electron-updater": "^4.6.1",
    "express": "^4.17.1",
    "express-jwt": "^6.1.0",
    "html-pdf": "^3.0.1",
    "jquery": "^3.6.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "ml-regression": "^5.0.0",
    "mongoose": "^6.0.5",
    "morgan": "^1.10.0",
    "nanoid": "^3.3.1",
